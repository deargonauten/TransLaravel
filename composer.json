{
    "name": "deargonauten/translaravel",
    "type": "library",
    "description": "A Laravel package which translates all. Routes, models and strings.",
    "keywords": [
        "deArgonauten",
        "TransLaravel",
        "Multilingual Laravel"
    ],
    "homepage": "https://github.com/deArgonauten/TransLaravel",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "license": "MIT",
    "authors": [
        {
            "name": "Jason de Ridder",
            "email": "mail@deargonauten.com"
        }
    ],
    "require": {
        "illuminate/support": "~5.1",
        "php" : ">=5.5|>=7.0"
    },
    "autoload": {
        "psr-4": {
            "deArgonauten\\TransLaravel\\": "src"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
