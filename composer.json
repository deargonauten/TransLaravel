{
    "name": "deargonauten/translaravel",
    "type": "library",
    "description": "A Laravel package which translates all. Routes, models (trait) and strings (gettext-style).",
    "keywords": [
        "Translation",
        "Translator",
        "TransLaravel",
        "Multilingual Laravel",
        "ISO 639-1",
        "Language",
        "i18n",
        "gettext"
    ],
    "homepage": "https://github.com/deArgonauten/TransLaravel",
    "license": "MIT",
    "authors": [
        {
            "name": "Jason de Ridder",
            "email": "mail@deargonauten.com"
        }
    ],
    "require": {
        "illuminate/support": "~5.1",
        "php" : ">=5.5|>=7.0"
    },
    "autoload": {
        "psr-4": {
            "deArgonauten\\TransLaravel\\": "src"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
