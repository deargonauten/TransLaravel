{
    "name": "deArgonauten/TransLaravel",
    "type": "library",
    "description": "A Laravel package which translates all. Routes, models and strings.",
    "keywords": [
        "deArgonauten",
        "TransLaravel",
        "Multilingual Laravel"
    ],
    "homepage": "https://github.com/deArgonauten/TransLaravel",
    "license": "MIT",
    "authors": [
        {
            "name": "Jason de Ridder",
            "email": "mail@deargonauten.com",
            "homepage": "http://www.deargonauten.com",
            "role": "Developer"
        }
    ],
    "require": {
        "illuminate/support": "~5.1",
        "php" : "~5.5|~7.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "~4.0||~5.0",
        "scrutinizer/ocular": "~1.1",
        "squizlabs/php_codesniffer": "~2.3"
    },
    "autoload": {
        "psr-4": {
            "deArgonauten\\TransLaravel\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "deArgonauten\\TransLaravel\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
